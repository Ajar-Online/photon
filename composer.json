{
  "name": "moeen/photon",
  "description": "Laravel Extension for SRP (Single Responsibility Principle)",
  "keywords": [
    "photon",
    "laravel",
    "lumen"
  ],
  "type": "library",
  "license": "MIT",
  "version": "0.2.0",
  "authors": [
    {
      "name": "Moeen Basra",
      "email": "m.basra@live.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "laravel/framework": "5.8.*"
  },
  "autoload": {
    "psr-4": {
      "Photon\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
